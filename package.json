{
  "name": "@capturedlabs/real-life-cdk-typescript",
  "version": "0.1.0",
  "description": "a collection of CDK 2 demo projects in TypeScript",
  "author": {
    "name": "Chris Chen",
    "email": "chris@capturedlabs.com",
    "url": "https://github.com/chrischenyc/real-life-cdk-typescript"
  },
  "private": true,
  "license": "BSD",
  "scripts": {
    "lint": "eslint packages",
    "lint:fix": "eslint packages --fix",
    "commit": "cz",
    "test": "lerna run test --stream",
    "test:ci": "lerna run test:ci --stream",
    "test:integration": "lerna run test:integration --stream",
    "build": "lerna run build --stream",
    "deploy": "lerna run deploy --stream",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.11.7",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "aws-cdk": "^2.49.0",
    "aws-cdk-lib": "^2.49.0",
    "constructs": "^10.1.143",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^7.0.0",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.1",
    "lerna": "^5.2.0",
    "prettier": "^2.6.0",
    "randomstring": "^1.2.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
